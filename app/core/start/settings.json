{
  "settings": {
    "version" : "v0.0.1", 
    "mod": 1,
    "defaultView": "clientes",
    "modCSS" : "dev",
    "userIcon" : "user-secret",
    "pulseInterval" : 12000,
    "tablePageSize" : 20,
    "serverPathPrd": "http://some/path/",
    "serverPath": "http://some/path/"
  },
  "routes" : [
    {
      "name" : "home", 
      "ngroute" : {
              "url": "/",
              "templateUrl": "app/welcome.html"
            },
      "modules" : ["*"]
    }, 
    {
      "name" : "blank", 
      "ngroute" : {
              "url": "blank",
              "templateUrl": "app/welcome.html"
            },
      "modules" : ["blank"]
    }, 
    {
      "name" : "clientes", 
      "ngroute" : {
              "url": "/clientes/:itemId/:searchCriteria/:sortField/:sortDirection/:page",
              "templateUrl": "app/biz/cliente/cliente.list.html",
              "controller" : "BizClienteListController"
              "reloadOnSearch" : false, 
              "params": {
                  "itemId": {
                      "value": "",
                      "squash": false
                  }, 
                  "searchCriteria" : {
                      "value" : "",
                      "squash" : false
                  },
                  "sortField": {
                      "value": "dateTime",
                      "squash": false
                  },
                  "sortDirection": {
                      "value": "desc",
                      "squash": false
                  },
                  "page": {
                      "value": "1",
                      "squash": false
                  }
              }
            },
        "modules" : ["admin", "dev"]
    }
  ]
}