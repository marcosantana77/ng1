{
  "config": {
    "title" : "NG1 - AngularJS Enterprise Scaffolding",
    "version" : "v0.0.1", 
    "mod": 1,
    "defaultView": "clientes",
    "modCSS" : "dev",
    "userIcon" : "user-secret",
    "pulseInterval" : 12000,
    "tablePageSize" : 20,
    "serverPathPrd": "http://some/path/",
    "serverPath": "http://some/path/"
  },
  "routes" : [
    {
      "name" : "home", 
      "ngroute" : {
              "url": "/home",
              "templateUrl": "/app/core/start/welcome.html"
            },
      "modules" : ["*"]
    }, 
    {
      "name" : "blank", 
      "ngroute" : {
              "url": "blank",
              "templateUrl": "/app/core/start/welcome.html"
            },
      "modules" : ["blank"]
    }, 
    {
      "name" : "shell", 
      "ngroute" : {
              "url": "shell",
              "templateUrl": "/app/core/shell/shell.html"
            },
      "modules" : ["*"]
    }, 
    {
      "name" : "clientes", 
      "ngroute" : {
              "url": "/clientes",
              "templateUrl": "app/biz/cliente/cliente.list.html",
              "controller" : "BizClienteListController as vm"
            },
        "modules" : ["admin", "dev"]
    }
  ]
}